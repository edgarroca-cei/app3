{
    "cli": {
        "version": ">= 10.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "env": {
                "EAS_CHANNEL": "development"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "env": {
                "EAS_CHANNEL": "preview"
            }
        },
        "production": {
            "distribution": "store",
            "channel": "production",
            "env": {
                "EAS_CHANNEL": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}